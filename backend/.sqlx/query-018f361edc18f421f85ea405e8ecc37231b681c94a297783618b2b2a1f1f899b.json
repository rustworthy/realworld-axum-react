  "query": "\n        SELECT\n            article.slug,\n            article.title,\n            article.description,\n            article.body,\n            article.tags,\n            article.created_at,\n            article.updated_at,\n            article.favorited_count,\n            author.username as author_username,\n            author.bio as author_bio,\n            author.image as author_image\n        FROM \"articles\" article\n        JOIN \"users\" author USING (user_id)\n        WHERE slug = $1;\n        ",
